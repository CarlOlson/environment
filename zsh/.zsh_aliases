alias ns='nix-shell --command zsh'
alias gst='git stash'
alias gs='git status'
alias gi='git update-index --skip-worktree'
alias dartium='dartium >/dev/null 2>&1'
alias rl='readlink -f'
alias ls='ls -G --color=auto'
alias zmv='noglob zmv'
alias zcp='noglob zmv -C'
alias zln='noglob zmv -L'
alias zsy='noglob zmv -Ls'
alias git_nuke='git clean -xfd'
alias gara='git commit --amend --reset-author'
alias find='noglob find'
alias untab='sed -i -e $'\''s/ / /g'\'' -e $'\''s/ \+$//g'\'
alias plowdown='noglob plowdown'
alias unrar='unrar x'
alias unzip='noglob unzip'
alias night='redshift -o -O 3500K'
